{
  version: 2,
  data: [
    {
      command_name: 'Copy to clopboard',
      key: 'ctrl+c',
      action: {
        type: 'power_shell',
      command: '\
Add-Type -AssemblyName System.Windows.Forms;\n\
$dataObj = New-Object System.Windows.Forms.DataObject;\n\
$dataObj.SetFileDropList($selecting_item_path_ary);\n\
$byteStream = [byte[]](([System.Windows.Forms.DragDropEffects]::Copy -as [byte]), 0, 0, 0);\n\
$memoryStream = [System.IO.MemoryStream]::new($byteStream);\n\
$dataObj.SetData("Preferred DropEffect", $memoryStream);\n\
[System.Windows.Forms.Clipboard]::SetDataObject($dataObj, $true);\n\
      ',
      },
      valid_on_addressbar: false,
    },
    {
      command_name: 'Cut to clopboard',
      key: 'ctrl+x',
      action: {
        type: 'power_shell',
      command: '\
Add-Type -AssemblyName System.Windows.Forms;\n\
$dataObj = New-Object System.Windows.Forms.DataObject;\n\
$dataObj.SetFileDropList($selecting_item_path_ary);\n\
$byteStream = [byte[]](([System.Windows.Forms.DragDropEffects]::Move -as [byte]), 0, 0, 0);\n\
$memoryStream = [System.IO.MemoryStream]::new($byteStream);\n\
$dataObj.SetData("Preferred DropEffect", $memoryStream);\n\
[System.Windows.Forms.Clipboard]::SetDataObject($dataObj, $true);\n\
      ',
      },
      valid_on_addressbar: false,
    },
    {
      command_name: 'Past from clopboard',
      key: 'ctrl+v',
      action: {
        type: 'power_shell',
      command: '\
Add-Type -AssemblyName System.Windows.Forms;\n\
\n\
$files = [Windows.Forms.Clipboard]::GetFileDropList() ;\n\
$data = [Windows.Forms.Clipboard]::GetDataObject();\n\
$dropEffect = $data.GetData("Preferred DropEffect");\n\
$flag = $dropEffect.ReadByte();\n\
\n\
if ($flag -band [Windows.Forms.DragDropEffects]::Copy) {\n\
    Copy-Item $files $current_dir;\n\
}\n\
if ($flag -band [Windows.Forms.DragDropEffects]::Move) {\n\
    Move-Item $files $current_dir;\n\
}\n\
      '
      },
      valid_on_addressbar: false,
    },
    {
      command_name: 'Create Copy',
      key: 'ctrl+shift+c',
      dialog_type: 'reference_selection',
      action: {
        type: 'power_shell',
      command: '\
for ($index=0; $index -lt $selecting_item_path_ary.count; $index++){\n\
  $selecting_item_path = $selecting_item_path_ary[$index];\n\
  $dialog_input_str = $dialog_input_str_ary[$index];\n\
  Copy-Item -Recurse -Path $selecting_item_path -Destination $dialog_input_str\n\
}\n\
      ',
      },
      valid_on_addressbar: false,
    },
    {
      command_name: 'Copy to opposite dirctory',
      key: 'ctrl+shift+c',
      dialog_type: 'none',
      action: {
        type: 'power_shell',
      command: '\
Copy-Item -Recurse -Path $selecting_item_path_ary -Destination $opposite_dir\n\
      ',
      },
      valid_on_addressbar: false,
    },
    {
      command_name: 'Delete file',
